{
  "name": "get-started-node",
  "main": "server.js",
  "description": "An introduction to developing Node.js apps on the Bluemix platform",
  "version": "0.1.1",
  "private": false,
  "engines": {
    "node": "7.*"
  },
  "scripts": {
    "answer": "cross-env DEBUG=true babel-node answer",
    "answer:watch": "cross-env DEBUG=true nodemon --exec 'babel-node' answer",
    "test": "jest",
    "test:watch": "jest --watch",
    "storage:watch": "cross-env DEBUG=true nodemon --exec 'babel-node' storage",
    "rabbitmq": "sudo rabbitmq-server",
    "rabbitmq:stop": "sudo rabbitmqctl stop",
    "reddis": "./binaries/redis-server"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "body-parser": "^1.16.1",
    "cfenv": "^1.0.x",
    "cloudant": "^1.7.1",
    "cross-env": "^4.0.0",
    "dotenv": "^4.0.0",
    "es6-promisify": "^5.0.0",
    "express": "^4.14.1",
    "ioredis": "^2.5.0",
    "json-stringify-deterministic": "^1.0.0",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "node-fetch": "^1.6.3",
    "uuid": "^3.0.1",
    "watson-developer-cloud": "^2.30.0"
  },
  "author": "Syrine AMIRA corp",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "ioredis-mock": "^2.4.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
